    "nodemon": "^2.0.7",
    "prettier": "^2.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "3.0.1",
    "relative-deps": "^1.0.7",
    "storybook-dark-mode": "^1.0.7"
